{
  "name": "JudgIt",
  "version": "0.1",
  "description": "judge pages",
  "manifest_version": 2,
  "permissions": [
    "activeTab",
    "tabs",
    "<all_urls>"
  ],
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "css": [
        "contentStyle.css"
      ],
      "js": [
        "contentScript.js"
      ]
    }
  ],
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "browser_action": {
    "default_popup": "popup.html"
  }
}